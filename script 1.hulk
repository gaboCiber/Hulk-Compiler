type Point {
    x = 3;
    y = 4;

    getX() => self.x;
    getY() => self.y;

    setX(x: Number): Number => self.x := x;
    setY(y: Number): Number => self.y := y;
};

type PolarPoint inherits Point {
    rho() => sqrt(self.getX() ^ 2 + self.getY() ^ 2);
};

let pt = new PolarPoint() in
{
    pt.setX(20);
    print(pt.getX());
    print(pt.rho());
};
    
