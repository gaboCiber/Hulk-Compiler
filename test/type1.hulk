type Point(x: Number,y: Number) {
    x = x;
    y = y;

    getX() => self.x;
    getY() => self.y;

    setX(x) => self.x := x;
    setY(y) => self.y := y;
};

let pt = new Point(3,5) in {
    print("x: " @ pt.getX() @ "; y: " @ pt.getY());
    pt.setX(10);
    print("x: " @ pt.getX() @ "; y: " @ pt.getY());
};
    